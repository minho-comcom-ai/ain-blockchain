{
  ".write": false,
  "accounts": {
    ".write": true
  },
  "afan": {
    ".write": true
  },
  "apps": {
    "chatbots": {
      ".write": true
    }
  },
  "consensus": {
    "number": {
      "$block_number": {
        "propose": {
          ".write": "data === null && newData !== null && util.isProposerForNumber($block_number, auth)"
        },
        "prevote": {
          "$uid": {
            ".write": "auth === $uid && data === null && newData !== null && util.isNumber(newData) && newData > 0 && util.isValidatorForNumber($block_number, auth)"
          }
        },
        "precommit": {
          "$uid": {
            ".write": "auth === $uid && data === null && newData !== null && util.isNumber(newData) && newData > 0 && util.isValidatorForNumber($block_number, auth)"
          }
        },
        "next_round_proposer": {
          ".write": "util.isProposerForNumber($block_number, auth)"
        },
        "next_round_validators": {
          ".write": "util.isProposerForNumber($block_number, auth)"
        }
      }
    }
  },
  "deposit": {
    "$service_id": {
      "$uid": {
        "$deposit_id": {
          "value": {
            ".write": "auth === $uid && !getValue('/deposit/' + $service_id + '/' + $uid + '/' + $deposit_id) && (util.length($uid) !== 42 || util.isCksumAddr($uid)) && util.isNumber(newData) && getValue('/accounts/' + $uid + '/balance') >= newData"
          },
          "createdAt": {
            ".write": false
          }
        }
      }
    }
  },
  "deposit_accounts": {
    ".write": false,
    "$service_id": {
      "config": {
        ".write": "!getValue('/deposit_accounts/' + $service_id) || (getOwner('/deposit_accounts/' + $service_id) && getOwner('/deposit_accounts/' + $service_id).owners && getOwner('/deposit_accounts/' + $service_id).owners[auth])"
      },
      "$uid": {
        ".write": false
      }
    },
    "consensus": {
      "config": {
        ".write": false
      }
    }
  },
  "transfer": {
    "$from": {
      "$to": {
        "$key": {
          "value": {
            ".write": "auth === $from && !getValue('transfer/' + $from + '/' + $to + '/' + $key) && (util.length($from) !== 42 || util.isCksumAddr($from)) && (util.length($to) !== 42 || util.isCksumAddr($to)) && $from !== $to && util.isNumber(newData) && getValue('accounts/' + $from + '/balance') >= newData"
          }
        }
      }
    }
  },
  "withdraw":{
    "$service_id": {
      "$uid": {
        "$withdraw_id": {
          "value": {
            ".write": "auth === $uid && !getValue('/withdraw/' + $service_id + '/' + $uid + '/' + $withdraw_id) && util.isNumber(newData) && getValue('/deposit_accounts/' + $service_id + '/' + $uid + '/value') >= newData"
          },
          "createdAt": {
            ".write": false
          }
        }
      }
    }
  }
}
